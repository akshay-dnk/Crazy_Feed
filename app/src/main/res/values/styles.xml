<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Style for icon material button with stroke -->
    <style name="CustomStrokeButtonIconStyle" parent="Widget.Material3.Button.OutlinedButton.Icon">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="iconSize">22dp</item>
        <item name="android:textColor">#1E1A16</item>
        <item name="iconTint">#212529</item>
        <item name="backgroundTint">@color/white</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">0dp</item>
        <item name="android:padding">0dp</item>
        <item name="strokeColor">#E9EBEC</item>
        <item name="strokeWidth">1dp</item>
        <item name="cornerRadius">11dp</item>
    </style>
</resources>